{
  "name": "foxmd",
  "version": "0.0.1",
  "description": "An opinionated Markdown to React transformer for React Server Component",
  "repository": {
    "url": "https://github.com/SukkaW/foxts"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "bunchee --minify --no-sourcemap"
  },
  "keywords": [
    "markdown",
    "react",
    "react-server-component",
    "rsc",
    "transformer",
    "marked",
    "marked-js"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "fast-escape-regexp": "^1.0.1",
    "foxts": "^5.0.2",
    "html-entities": "^2.6.0",
    "marked": "^17.0.1",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "bumpp": "^10.3.2",
    "bunchee": "^6.6.2",
    "eslint": "^9.39.1",
    "eslint-config-sukka": "^8.0.5",
    "eslint-formatter-sukka": "^8.0.5",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@types/react": "*"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "overrides": {
      "eslint>chalk": "npm:ansis@^3.17.0"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "oxc-resolver",
      "unrs-resolver"
    ]
  }
}
