{
  "name": "foxmd",
  "version": "0.2.0-beta.17",
  "description": "An opinionated Markdown to React transformer for React Server Component",
  "repository": {
    "url": "https://github.com/SukkaW/foxmd"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "bunchee --minify --no-sourcemap",
    "prerelease": "pnpm run lint && pnpm run build",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "markdown",
    "react",
    "react-server-component",
    "rsc",
    "transformer",
    "marked",
    "marked-js"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "domhandler": "^5.0.3",
    "fast-escape-regexp": "^1.0.1",
    "foxts": "^5.0.4",
    "html-entities": "^2.6.0",
    "htmlparser2": "^10.0.0",
    "marked": "^15.0.12",
    "postcss": "^8.5.6",
    "postcss-js": "^5.0.3"
  },
  "devDependencies": {
    "@types/postcss-js": "^4.1.0",
    "@types/react": "^19.2.7",
    "bumpp": "^10.3.2",
    "bunchee": "^6.8.0",
    "eslint": "^9.39.2",
    "eslint-config-sukka": "^8.0.6",
    "eslint-formatter-sukka": "^8.0.6",
    "html-tags": "^5.1.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": "*"
  },
  "packageManager": "pnpm@10.26.2",
  "pnpm": {
    "overrides": {
      "eslint>chalk": "npm:ansis@3.17.0"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "oxc-resolver",
      "unrs-resolver"
    ]
  }
}
